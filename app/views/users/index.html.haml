- single_column do
  = paginate(@users)

  .listing
    %table{:cellpadding => 0, :cellspacing => 0, :border => 0}
      %tr.header
        %td Name 
        %td Email
        %td Workspaces
      - @users.each do |user|
        %tr
          %td= render_user(user)
          %td= render_email(user.email)
          %td

  = paginate(@users)
